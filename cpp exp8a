
#include <iostream>

using namespace std;

int main() 
{
	string s1,s2,result;
	cout<<"Enter string s1: "<<endl;
	cin>>s1;
	cout<<"Enter string s2: "<<endl;
	cin>>s2;
	result = s1 + s2;
	cout<<"Result string: "<<result;
}
/*output
Enter string s1:
hello
Enter string s2:
world
Result string: helloworld */
